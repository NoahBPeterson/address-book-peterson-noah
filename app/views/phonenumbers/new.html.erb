<h1>Add new Phone Number</h1>

<%= form_for @phonenumber ,:url => {:action => :create, :controller => :phonenumbers} do |f| %>
    <%= f.label :phone_number%>
    <%= f.text_field :phone_number%>
    <%= f.label :comment%>
    <%= f.text_field :comment%>
    <%= f.hidden_field :person_id, value: params[:person_id] %>
    <%= f.submit%>
<%end%>